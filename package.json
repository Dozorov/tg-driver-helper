{
  "name": "tg-driver-helper",
  "version": "1.0.0",
  "description": "Telegram driver onboarding and helper bot with AI integration",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "watch": "nodemon",
    "dev:watch": "nodemon --watch src --ext ts,js,json --exec ts-node src/index.ts",
    "dev:debug": "nodemon --inspect --exec ts-node src/index.ts",
    "setup": "node scripts/setup.js"
  },
  "keywords": [
    "telegram",
    "bot",
    "driver",
    "onboarding",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.38.4",
    "@types/sqlite3": "^3.1.11",
    "aws-sdk": "^2.1490.0",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "isomorphic-fetch": "^3.0.0",
    "joi": "^17.11.0",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.7.0",
    "openai": "^4.20.1",
    "postgres": "^3.4.7",
    "sqlite3": "^5.1.7",
    "telegraf": "^4.15.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/multer": "^1.4.9",
    "@types/node": "^20.8.10",
    "@types/uuid": "^9.0.6",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
